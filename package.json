{
  "name": "tsumuchicken",
  "version": "1.0.0",
  "description": "juego feito",
  "main": "./dist/electron-main.js",
  "scripts": {
    "start": "electron .",
    "setup": "npm install",
    "serve": "cross-env BUILD_ENV=development APP_ENV=development  webpack-dev-server --open ",
    "serve:min": "cross-env BUILD_ENV=production APP_ENV=development webpack-dev-server --optimize-minimize  --open ",
    "build:dev": "cross-env BUILD_ENV=development APP_ENV=development webpack  ",
    "build:sand": "cross-env BUILD_ENV=sandbox APP_ENV=sandbox webpack  ",
    "build:prod": "cross-env BUILD_ENV=production APP_ENV=production webpack --optimize-minimize",
    "build:sand:min": "cross-env BUILD_ENV=production APP_ENV=sandbox webpack  --optimize-minimize",
    "build:prod:debug": "cross-env BUILD_ENV=sandbox APP_ENV=production webpack  ",
    "build-analyzer-stats": "cross-env BUILD_ENV=sandbox APP_ENV=sandbox webpack --profile --json > stats.json",
    "build-analyzer-stats:min": "cross-env BUILD_ENV=production APP_ENV=production webpack --optimize-minimize --profile --json > stats.json",
    "show-bundle-stats": "webpack-bundle-analyzer stats.json",
    "release-mac": "electron-packager . --overwrite --platform=darwin --arch=x64  --prune=true --out=release-builds"
  },
  "author": "martelgames@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^14.14.19",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.8.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "electron": "^11.1.1",
    "electron-packager": "^15.2.0",
    "electron-webpack": "^2.8.2",
    "eslint": "^7.17.0",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "jsonfile": "^6.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "raw-loader": "^4.0.2",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^1.4.3",
    "to-string-loader": "^1.1.6",
    "ts-loader": "^8.0.13",
    "tslint": "^6.1.3",
    "tslint-loader": "^3.5.4",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "yargs": "^16.2.0"
  },
  "dependencies": {
    "@pixi/unsafe-eval": "^5.3.7",
    "pixi-sound": "^3.0.5",
    "pixi.js": "^5.3.7"
  }
}
